{
  "extends": ["<%= style %>", "stylelint-config-prettier"],
  "rules": {
    "at-rule-no-unknown": [ true, {
      "ignoreAtRules": [
        "extends",
        "ignores"
      ]
    }],
    "no-empty-source": null,
    "indentation": null,
    "number-leading-zero": null,
    "unit-whitelist": ["em", "rem", "s", "%"]
  }
}
